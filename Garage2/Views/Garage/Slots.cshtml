@model IEnumerable<Garage2.Models.ParkingSlot>

@{
    ViewBag.Title = "Slots";
}

<h2>Slots</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Occupied)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VehicleReg)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Occupied)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VehicleReg)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
